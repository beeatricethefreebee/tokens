{
  "core": {
    "spacing": {
      "xs": {
        "$type": "spacing",
        "$value": "{dimension.xs}"
      },
      "sm": {
        "$type": "spacing",
        "$value": "{dimension.sm}"
      },
      "md": {
        "$type": "spacing",
        "$value": "{dimension.md}"
      },
      "lg": {
        "$type": "spacing",
        "$value": "{dimension.lg}"
      },
      "xl": {
        "$type": "spacing",
        "$value": "{dimension.xl}"
      },
      "multi-value": {
        "$type": "spacing",
        "$value": "{dimension.sm} {dimension.xl}",
        "$description": "You can have multiple values in a single spacing token. Read more on these: https://docs.tokens.studio/available-tokens/spacing-tokens#multi-value-spacing-tokens"
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "core"
    ]
  }
}